{
  "name": "Document Parser",
  "nodes": [
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "parse-document",
        "responseMode": "responseNode"
      },
      "id": "webhook-trigger",
      "name": "Document Upload Webhook",
      "type": "n8n-nodes-base.webhook",
      "position": [240, 300]
    },
    {
      "parameters": {
        "url": "http://localhost:11434/api/generate",
        "options": {
          "timeout": 45000
        },
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "model",
              "value": "qwen2.5:1.5b"
            },
            {
              "name": "prompt",
              "value": "={{`You are an expert document analyzer specializing in GST-related documents. Analyze this document: ${$json.body.documentText} Provide: 1. Document Type 2. Key Information (dates, amounts, GSTIN, notice numbers) 3. Issues Identified 4. Action Required 5. Timeline and deadlines 6. Legal Provisions referenced. Give structured analysis for further processing.`}}"
            },
            {
              "name": "stream",
              "value": false
            }
          ]
        }
      },
      "id": "ollama-parse-request",
      "name": "Parse Document",
      "type": "n8n-nodes-base.httpRequest",
      "position": [460, 300]
    },
    {
      "parameters": {
        "respondWith": "json",
        "responseBody": "={{ { analysis: $json.response, source: 'Document AI + Qwen2.5', timestamp: $now, processed: true } }}"
      },
      "id": "respond-to-webhook",
      "name": "Return Analysis",
      "type": "n8n-nodes-base.respondToWebhook",
      "position": [680, 300]
    }
  ],
  "connections": {
    "Document Upload Webhook": {
      "main": [
        [
          {
            "node": "Parse Document",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Parse Document": {
      "main": [
        [
          {
            "node": "Return Analysis",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}
